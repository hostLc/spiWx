page{
  height:100%;
}
.orderinfo_page{
  height:100%;
  overflow: hidden;
}

.orderinfo-list{
  display:flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  height:110rpx;
  margin-left:40rpx;
  padding-right:46rpx;
  border-bottom:1px solid #e5e5e5;
}
.item-title{
  display: block;
  height:16px;
  line-height:16px;
  font-size:14px;
  margin-top:28rpx;
  padding:0;
  margin-bottom:0;
  color:#333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;

}
.item-time{
  line-height:12px;
  font-size:12px;
  color:#999;
  margin:0;padding:0;
}
.list-status{
  height:110rpx;
  line-height:110rpx;
  font-size:14px;
  color:#333;
}
.list-status.ons{
  color:#125fa3;
}